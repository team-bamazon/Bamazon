<%= render partial: 'layouts/header' %>

<div id = "yourOrders" role = "main">
  <div id = "yourOrdersContent">
    <div class="a-section a-spacing-large a-spacing-top-small a-subheader a-breadcrumb">
    <ul class="a-unordered-list a-nostyle a-horizontal">
      <li>
        <span class="a-list-item">
          <a class="a-link-normal" title="アカウントサービスに戻る" href="/users">
            <span>
            アカウントサービス
            </span>
          </a>
        </span>
      </li>
      <li class="a-breadcrumb-divider">›</li>
      <li>
        <span class="a-list-item">
          <span class="a-color-state">
            注文履歴
          </span>
        </span>
      </li>
    </ul>
  </div>
  <div class="a-row">
    <div class="a-column a-span6">
      <h1 class="a-spacing-medium">
        注文履歴
      </h1>
    </div>
    <div class="a-column a-span6 a-span-last">
      <div class="a-row a-spacing-medium search-bar">
        <form id="searchForm" method="get" action="/gp/your-account/order-history/ref=oh_aui_search" class="form-container">
          <input type="hidden" name="opt" value="ab">
          <input type="hidden" name="__mk_ja_JP" value="カタカナ">
          <div class="a-row field-container">
            <label for="searchOrdersInput" class="a-form-label label">注文を検索：
            </label>
            <div class="a-search">
              <i class="a-icon a-icon-search"></i>
              <input type="search" id="searchOrdersInput" placeholder="タイトル、カテゴリー、またはお届け先で検索" name="search" class="a-input-text field">
            </div>
          </div>
          <div class="a-row button-container">
            <span class="a-button a-button-search search-order-input" id="a-autoid-7">
              <span class="a-button-inner">
                <input class="a-button-input" type="submit" aria-labelledby="a-autoid-7-announce">
                <span class="a-button-text" aria-hidden="true" id="a-autoid-7-announce">
                  注文を検索
                </span>
              </span>
            </span>
          </div>
        </form>
      </div>
    </div>
  </div>
  <div id="controlsContainer">
    <div id="orderTypeMenuContainer" class="a-row a-spacing-medium custom-view-options">
      <ul class="a-unordered-list a-nostyle a-horizontal" role="tablist">
        <li role="tab">
          <span class="a-list-item">
          </span>
        </li>
        <li class="selected" role="tab">
          <span class="a-list-item">
            <span class="item">
              注文
            </span>
          </span>
        </li>
        <li role="tab">
          <span class="a-list-item">
            <a class="a-link-normal item" href="/gp/your-account/order-history/ref=oh_aui_menu_open?ie=UTF8&amp;orderFilter=open">未発送の注文
            </a>
          </span>
        </li>
        <li role="tab"><span class="a-list-item">
          <a class="a-link-normal item" href="/gp/your-account/order-history/ref=oh_aui_menu_cancelled?ie=UTF8&amp;orderFilter=cancelled">
            キャンセルされた注文
          </a>
        </span>
      </li>
    </ul>
  </div>
  <div class="a-row a-spacing-base top-controls">
    <span class="a-declarative" data-action="time-period-change" data-time-period-change="{}">
      <form id="timePeriodForm" method="get" action="/gp/your-account/order-history" class="time-period-chooser a-spacing-none">
        <input type="hidden" name="opt" value="ab">
        <input type="hidden" name="digitalOrders" value="1">
        <input type="hidden" name="unifiedOrders" value="1">
        <input type="hidden" name="returnTo">
        <input type="hidden" name="__mk_ja_JP" value="カタカナ">
        <span class="a-dropdown-container">
          <select name="orderFilter" autocomplete="off" id="orderFilter" tabindex="-1" class="a-native-dropdown">
            <option value="last30" id="orderFilterEntry-last30">過去30日間
            </option>
            <option value="months-6" id="orderFilterEntry-months-6">過去6カ月間
            </option>
            <option value="year-2017" id="orderFilterEntry-year-2017" selected="">
              2017年
            </option>
            <option value="year-2016" id="orderFilterEntry-year-2016">
                2016年
            </option>
            <option value="year-2015" id="orderFilterEntry-year-2015">
                2015年
            </option>
            <option value="year-2014" id="orderFilterEntry-year-2014">
                2014年
            </option>
            <option value="year-2013" id="orderFilterEntry-year-2013">
                2013年
            </option>
            <option value="year-2012" id="orderFilterEntry-year-2012">
                2012年
            </option>
            <option value="year-2011" id="orderFilterEntry-year-2011">
                2011年
            </option>
          </select>
        </span>
      </form>
    </span>
    <label for="orderFilter" class="a-form-label num-orders-for-orders-by-date aok-inline-block a-text-normal">に確定された<span class="num-orders">9件</span>の注文
    </label>
  </div>
  </div>
  <div id = "ordersContainer">
    <% @orders.each do |order| %>
    <div class="a-box-group a-spacing-base order">
      <div class="a-box a-color-offset-background order-info">
        <div class="a-box-inner">
          <div class="a-fixed-right-grid">
            <div class="a-fixed-right-grid-inner" style="padding-right:290px">
              <div class="a-fixed-right-grid-col a-col-left" style="padding-right:0%;float:left;">
                <div class="a-row">
                  <div class="a-column a-span3">
                    <div class="a-row a-size-mini">
                      <span class="a-color-secondary label">
                      注文日
                      </span>
                    </div>
                    <div class="a-row a-size-base">
                      <span class="a-color-secondary value">
                        <%= order.created_at %>
                      </span>
                    </div>
                  </div>
                  <div class="a-column a-span2">
                    <div class="a-row a-size-mini">
                      <span class="a-color-secondary label">
                      合計
                      </span>
                    </div>
                    <div class="a-row a-size-base">
                      <span class="a-color-secondary value">
                      ￥ <%= order.order_price_sum(order)%>
                      </span>
                    </div>
                  </div>
                  <div class="a-column a-span7 recipient a-span-last">
                    <div class="a-row a-size-mini">
                      <span class="a-color-secondary label">
                      お届け先
                      </span>
                    </div>
                    <div class="a-row a-size-base">
                      <span class="a-color-secondary">
                        <span class="a-declarative" style="">
                          <a href="javascript:void(0)" class="a-popover-trigger a-declarative value"><span class="trigger-text">
                          貝塚秀雄
                          </span>
                          <i class="a-icon a-icon-popover"></i>
                        </a>
                      </span>
                    </span>
                  </div>
                </div>
              </div>
            </div>
            <div class="a-fixed-right-grid-col actions a-col-right" style="width:290px;margin-right:-290px;float:left;">
              <div class="a-row a-size-mini">
                <span class="a-color-secondary label">
                注文番号
              </span>
              <span class="a-color-secondary value">
              <%= order.order_number(order) %>
              </span>
            </div>
            <div class="a-row a-size-base">
              <ul class="a-unordered-list a-nostyle a-vertical">
                <a class="a-link-normal" href="/orders">
                注文の詳細
                </a>
                <i class="a-icon a-icon-text-separator"></i>
                <span class="hide-if-js">
                  <a class="a-link-normal" href="/orders">
                  領収書／購入明細書
                </a>
              </span>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% order.order_products.each do |order_product| %>
